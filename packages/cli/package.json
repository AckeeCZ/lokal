{
  "name": "lokse",
  "description": "Tool to efficient usage of translations stored in google spreadsheet",
  "version": "2.1.4",
  "author": {
    "name": "Lukas Horak",
    "email": "horaklukas@centrum.cz"
  },
  "bin": {
    "lokse": "./bin/run"
  },
  "bugs": "https://github.com/AckeeCZ/lokse/issues",
  "dependencies": {
    "@lokse/core": "^2.1.4",
    "@oclif/core": "^1.9.2",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-version": "^1.1.0",
    "@sindresorhus/slugify": "^1.1.0",
    "@types/array.prototype.flat": "^1.2.1",
    "@types/dedent": "^0.7.0",
    "@types/inquirer": "^8.1.3",
    "@types/update-notifier": "^5.0.0",
    "array.prototype.flat": "^1.2.4",
    "dedent": "^0.7.0",
    "dotenv-flow": "^3.2.0",
    "inquirer": "^8.2.0",
    "open": "^7.2.1",
    "ora": "^5.0.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tslib": "^1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "oclif": "^3",
    "@oclif/test": "^2",
    "@types/jest-when": "^2.7.2",
    "@types/node": "^12",
    "jest-when": "^3.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/AckeeCZ/lokse",
  "keywords": [
    "cli",
    "localization",
    "spreadsheet"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lokse",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "AckeeCZ/lokse",
  "scripts": {
    "build": "tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && yarn build && oclif manifest && oclif readme",
    "version": "yarn readme && git add README.md",
    "readme": "oclif readme",
    "release": "yarn version"
  },
  "types": "lib/index.d.ts"
}
